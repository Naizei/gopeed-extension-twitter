{
  "name": "twitter",
  "author": "monkeyWie",
  "title": "Twitter",
  "description": "Twitter media download, support video, gif, image, etc.",
  "icon": "icon.png",
  "version": "1.2.0",
  "homepage": "https://github.com/monkeyWie/gopeed-extension-twitter",
  "repository": {
    "url": "https://github.com/monkeyWie/gopeed-extension-twitter"
  },
  "scripts": [
    {
      "event": "onResolve",
      "match": {
        "urls": ["*://twitter.com/*/status/*", "*://x.com/*/status/*"]
      },
      "entry": "dist/index.js"
    }
  ],
  "settings": [
    {
      "name": "cookie",
      "title": "Cookie",
      "description": "Twitter cookie",
      "type": "string"
    }
  ]
}
